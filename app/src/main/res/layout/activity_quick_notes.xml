<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fdfcdc"
    android:visibility="visible"
    tools:context=".QuickNotes"
    tools:visibility="visible">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#fdfcdc"
        android:visibility="visible"
        tools:context=".QuickNotes"
        tools:visibility="visible">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#fdfcdc"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:context=".QuickNotes"
            tools:visibility="visible">

            <EditText
                android:id="@+id/editTextNote3"
                android:layout_width="397dp"
                android:layout_height="100dp"
                android:layout_marginStart="7dp"
                android:layout_marginTop="345dp"
                android:layout_marginEnd="7dp"
                android:layout_marginBottom="400dp"
                android:background="#FFFFFF"
                android:ems="10"
                android:hint="...I need to remember..."
                android:inputType="textPersonName"
                android:textColor="#000000"
                android:textSize="20sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                app:layout_constraintVertical_bias="0.0"
                tools:visibility="visible" />

            <EditText
                android:id="@+id/editTextNote"
                android:layout_width="397dp"
                android:layout_height="100dp"
                android:layout_marginStart="7dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="7dp"
                android:layout_marginBottom="420dp"
                android:background="#FFFFFF"
                android:ems="10"
                android:hint="...I need to remember..."
                android:inputType="textPersonName"
                android:textColor="#000000"
                android:textSize="20sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                app:layout_constraintVertical_bias="1.0"
                tools:visibility="visible" />

            <EditText
                android:id="@+id/editTextNote1"
                android:layout_width="397dp"
                android:layout_height="100dp"
                android:layout_marginStart="7dp"
                android:layout_marginTop="150dp"
                android:layout_marginEnd="7dp"
                android:layout_marginBottom="350dp"
                android:background="#FFFFFF"
                android:ems="10"
                android:hint="...I need to remember..."
                android:inputType="textPersonName"
                android:textColor="#000000"
                android:textSize="20sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                app:layout_constraintVertical_bias="0.39"
                tools:visibility="visible" />

            <EditText
                android:id="@+id/editTextNote2"
                android:layout_width="397dp"
                android:layout_height="100dp"
                android:layout_marginStart="7dp"
                android:layout_marginTop="235dp"
                android:layout_marginEnd="7dp"
                android:layout_marginBottom="300dp"
                android:background="#FFFFFF"
                android:ems="10"
                android:hint="...I need to remember..."
                android:inputType="textPersonName"
                android:textColor="#000000"
                android:textSize="20sp"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3"
                app:layout_constraintVertical_bias="0.0"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="415dp"
                android:layout_height="66dp"
                android:background="#00BCD4"
                android:text="Quick Notes!"
                android:textAlignment="center"
                android:textSize="40sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/editTextNote"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>